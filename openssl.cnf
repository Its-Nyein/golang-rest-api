[ req ]
default_bits       = 2048
distinguished_name = req_distinguished_name
x509_extensions    = v3_ca
prompt             = no

[ req_distinguished_name ]
C  = US
ST = California
L  = San Francisco
O  = Example Company
OU = IT
CN = localhost

[ v3_ca ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = localhost
